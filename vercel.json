{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/socket.io",
      "dest": "node_modules/socket.io-client/dist"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
