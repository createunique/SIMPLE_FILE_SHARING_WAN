{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs14.x"
    }
  },
  "env": {
    "PORT": 3000
  },
  "dependencies": {
    "express": "^4.19.1",
    "socket.io": "^4.7.5"
  }
}

